.c2 .token.keyword {
  color: #07a;
}

.c2 .token.function,
.c2 .token.class-name {
  color: #dd4a68;
}

.c2 .token.regex,
.c2 .token.important,
.c2 .token.variable {
  color: #e90;
}

.c2 .token.important,
.c2 .token.bold {
  font-weight: bold;
}

.c2 .token.italic {
  font-style: italic;
}

.c2 .token.entity {
  cursor: help;
}

.c2 * {
  box-sizing: border-box;
}

.c2 code {
  background-color: #f4f4f4;
  padding: 0.2em 0px;
  margin: 0px;
  border-radius: 3px;
  line-height: 1.2;
}

.c2 p {
  margin-top: 0;
  margin-bottom: 8px;
}

.c2 p:last-child {
  margin-bottom: 0 !important;
}

.c2 pre {
  background-color: #f4f4f4;
}

.c2 pre > code {
  display: block;
  background: none;
  overflow-y: auto;
}

.c2 code,
.c2 kbd,
.c2 pre {
  font-family: 'Ubuntu Mono','Courier New',monospace;
}

.c2 kbd {
  display: inline-block;
  padding: 3px 5px;
  font-size: 11px;
  line-height: 10px;
  color: #2A506F;
  vertical-align: middle;
  background-color: rgba(244,244,244,0.7);
  border: solid 1px #c6c8c9;
  border-bottom-color: hsl(200,2.7%,70.4%);
  border-radius: 3px;
  box-shadow: inset 0 -1px 0 hsl(200,2.7%,70.4%);
}

.c2 dl {
  padding: 0;
}

.c2 dl dt {
  padding: 0;
  margin-top: 16px;
  font-size: 1em;
  font-style: italic;
  font-weight: 600;
}

.c2 dl dd {
  padding: 0 16px;
  margin-bottom: 16px;
}

.c2 td,
.c2 th {
  padding: 0;
}

.c2 table {
  border-spacing: 0;
  border-collapse: collapse;
  display: block;
  width: 100%;
  overflow: auto;
}

.c2 table th {
  font-weight: 600;
}

.c2 table th,
.c2 table td {
  padding: 6px 13px;
  border: 1px solid #c6c8c9;
}

.c2 table tr {
  background-color: #fff;
  border-top: 1px solid #f4f4f4;
}

.c2 table tr:nth-child(2n) {
  background-color: #f4f4f4;
}

.c2 blockquote {
  margin: 0;
}

.c2 blockquote,
.c2 dl,
.c2 table {
  margin-top: 0;
  margin-bottom: 16px;
}

.c2 blockquote {
  padding: 0 1em;
  color: #2A506F;
  border-left: 0.25em solid #c6c8c9;
}

.c2 blockquote > :first-child {
  margin-top: 0;
}

.c2 blockquote > :last-child {
  margin-bottom: 0;
}

.c2 .heading-anchor-link span.icon {
  background-image: url('data:image/png;base64,');
  width: 20px;
  background-repeat: no-repeat;
  height: 20px;
  display: inline-block;
  -webkit-transform-origin: 0px 0px;
  -ms-transform-origin: 0px 0px;
  transform-origin: 0px 0px;
  visibility: hidden;
}

.c2 .heading-anchor-link {
  position: absolute;
  top: 0;
  left: 0;
  -webkit-transform: translateX(-100%);
  -ms-transform: translateX(-100%);
  transform: translateX(-100%);
  width: 20px;
  height: 20px;
  padding-right: 4px;
}

.c2 h1:hover .heading-anchor-link span.icon,
.c2 h2:hover .heading-anchor-link span.icon,
.c2 h3:hover .heading-anchor-link span.icon,
.c2 h4:hover .heading-anchor-link span.icon,
.c2 h5:hover .heading-anchor-link span.icon,
.c2 h6:hover .heading-anchor-link span.icon {
  visibility: visible;
}

.c2 h1,
.c2 h2,
.c2 h3,
.c2 h4,
.c2 h5,
.c2 h6 {
  position: relative;
}

@media print {
  .c2 code[class*='language-'],.c2 pre[class*='language-'] {
    text-shadow: none;
  }
}

<div>
  <div
    class="c0 c1"
  >
    <div
      class="c2"
    >
      <div>
        <p
          class=""
        />
      </div>
    </div>
  </div>
</div>
`;
