{
  "name": "resin-components",
  "version": "1.3.4",
  "description": "Resin Components Component",
  "repository": {
    "type": "git",
    "url": "https://github.com/resin-io-modules/resin-components"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "watch": "babel src -w -d dist",
    "build": "babel src -d dist",
    "test": "npm run lint && mocha --compilers js:babel-core/register test/*.spec.js",
    "lint": "eslint src test",
    "precommit": "lint-staged",
    "prettify": "prettier-eslint --write \"src/**/*.js\" \"test/**/*.js\"",
    "publish-storybook": "bash .scripts/publish_storybook.sh"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.4.0",
    "@storybook/addon-info": "3.2.9",
    "@storybook/react": "3.2.8",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.5",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "git-url-parse": "^7.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.1",
    "mocha": "^3.5.0",
    "preact-compat": "^3.14.1",
    "prettier-eslint-cli": "4.3.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "dependencies": {
    "color": "^2.0.0",
    "grid-styled": "2.0.0-10",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "preact": "^7.2.0",
    "react-icons": "^2.2.5",
    "recompose": "0.25.0",
    "regex-parser": "^2.2.7",
    "resin-semver": "^0.10.0",
    "showdown": "^1.7.3",
    "styled-grid": "1.0.0-beta.1",
    "styled-system": "1.0.0-13",
    "tag-hoc": "1.0.0",
    "paths-js": "^0.4.5",
    "resin-device-status": "^1.1.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "npm run lint",
      "git add"
    ]
  }
}