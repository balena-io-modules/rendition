{
  "name": "rendition",
  "version": "20.18.1",
  "description": "React components library",
  "repository": {
    "type": "git",
    "url": "https://github.com/balena-io-modules/rendition"
  },
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist_esm5/index.js",
  "sideEffects": false,
  "files": [
    "dist/",
    "dist_esm5/",
    "docs/",
    "README.md"
  ],
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=5.7.0"
  },
  "scripts": {
    "tsify-css": "node ./scripts/tsifyCss.js",
    "dev": "npm run build && npm run start",
    "start": "start-storybook -p 6006",
    "build:cjs": "rimraf dist && tsc",
    "build:esm5": "rimraf dist_esm5 && tsc -p ./tsconfig.esm5.json",
    "build": "npm run tsify-css && npm run build:cjs && npm run build:esm5",
    "build:storybook": "build-storybook -o .out",
    "generate-snapshots": "npm run build && npm run jest -- -u",
    "test:fast": "npm run lint && npm run jest",
    "test:fast:watch": "npm run lint && npm run jest:watch",
    "test": "npm run build && npm run test:fast && npm run catch-uncommitted",
    "jest": "jest --maxWorkers=4",
    "jest:watch": "jest --maxWorkers=4 --watch",
    "lint:fix": "balena-lint --fix src",
    "lint": "balena-lint src",
    "catch-uncommitted": "catch-uncommitted --catch-no-git --skip-node-versionbot-changes",
    "precommit": "lint-staged",
    "prepack": "npm run build",
    "prettify": "npm run lint:fix",
    "deploy": "npm run build:storybook && gh-pages -d .out"
  },
  "devDependencies": {
    "@balena/lint": "^6.1.1",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addon-storyshots": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.0",
    "@types/enzyme": "^3.10.8",
    "@types/googlemaps": "3.39.7",
    "@types/highlightjs": "^9.12.0",
    "@types/jest": "^26.0.21",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.11",
    "@types/react-google-recaptcha": "^1.1.2",
    "@types/sinon": "^9.0.8",
    "@types/unist": "^2.0.3",
    "catch-uncommitted": "^1.4.0",
    "core-js": "^3.7.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "gh-pages": "2.0.0",
    "git-url-parse": "^7.0.0",
    "husky": "^0.14.3",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^11.6.2",
    "lint-staged": "^4.1.1",
    "match-media-mock": "^0.1.1",
    "monaco-editor-webpack-plugin": "^4.0.0",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "sinon": "^4.5.0",
    "strip-css-comments": "^3.0.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.6",
    "zxcvbn": "^4.4.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@mapbox/rehype-prism": "^0.5.0",
    "@react-google-maps/api": "^1.13.0",
    "@rjsf/core": "^2.2.1",
    "@types/ajv-keywords": "^3.4.0",
    "@types/color": "^3.0.0",
    "@types/history": "^4.7.8",
    "@types/json-schema": "^7.0.5",
    "@types/lodash": "4.14.165",
    "@types/node": "^14.14.35",
    "@types/prop-types": "^15.7.0",
    "@types/react-helmet": "^6.1.0",
    "@types/recompose": "^0.26.2",
    "@types/styled-components": "^5.1.9",
    "@types/styled-system": "^4.0.0",
    "@types/uuid": "^3.4.3",
    "ajv": "^6.12.3",
    "ajv-keywords": "^3.5.2",
    "analytics-client": "^1.6.0",
    "color": "^3.1.3",
    "color-hash": "^1.0.3",
    "copy-to-clipboard": "^3.0.8",
    "date-fns": "^2.16.1",
    "grommet": "^2.16.3",
    "hast-util-sanitize": "^3.0.0",
    "json-e": "^4.1.0",
    "lodash": "^4.17.21",
    "mermaid": "^8.8.3",
    "prismjs": "^1.21.0",
    "prop-types": "^15.7.2",
    "qs": "^6.10.1",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^6.0.0",
    "react-monaco-editor": "^0.40.0",
    "react-notifications-component": "^2.4.1",
    "react-simplemde-editor": "^4.1.3",
    "recompose": "0.26.0",
    "regex-parser": "^2.2.11",
    "regexp-match-indices": "^1.0.2",
    "rehype-raw": "^5.0.0",
    "rehype-react": "^6.1.0",
    "rehype-sanitize": "^3.0.1",
    "remark-breaks": "^2.0.1",
    "remark-parse": "^8.0.3",
    "remark-rehype": "^7.0.0",
    "resize-observer": "^1.0.0",
    "skhema": "^5.3.4",
    "styled-components": "^5.2.1",
    "styled-system": "^4.1.0",
    "tslib": "^2.1.0",
    "unified": "^9.1.0",
    "unist-util-visit-parents": "^3.1.0",
    "uuid": "^3.2.1",
    "xterm": "^4.8.1",
    "xterm-addon-fit": "^0.4.0"
  },
  "lint-staged": {
    "{package.json, package-lock.json}": [
      "npm run tsify-css",
      "git add ./src/components/Terminal/XTermDefaultStyle.ts"
    ],
    "*.{ts,tsx}": [
      "balena-lint --fix",
      "git add"
    ]
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.stories\\.(jsx?|tsx?)$": "@storybook/addon-storyshots/injectFileName",
      "^.+\\.(jsx?|tsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(entity-decode|mermaid|monaco-editor|react-monaco-editor)/)"
    ],
    "testRegex": ".*spec.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>scripts/setupTests.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>scripts/setupTestsAfterEnv.js"
    ]
  }
}
